{
    "version": 2,
    "builds": [
      {
        "src": "./routes/*.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/"
        }
    ],
    "routes": [
      {
        "src": "/routes/(.*)",
        "dest": "/routes/$1.js"
      }
    ]
  }
  